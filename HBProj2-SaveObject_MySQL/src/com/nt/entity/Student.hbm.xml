<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
		<class name="com.nt.entity.Student" table="student_registration" dynamic-insert="true">
			<id name="sno" column="SNO">
				<generator class="increment"></generator>
			</id>
			<property name="sname" column="SNAME"/>
			<property name="sadd" column="SADD"/>
			<property name="dob" column="DOB"/>
			<property name="smark" column="SMARK"/>
			<property name="tmark" column="TMARK"/>
			<property name="cgpa" column="CGPA"/>
		</class>
	</hibernate-mapping>